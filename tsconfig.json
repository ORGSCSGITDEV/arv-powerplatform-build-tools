{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./out",
    "resolveJsonModule": true,
    "strict": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": true,
  },
  "exclude": [
    "node_modules"
  ]
}
